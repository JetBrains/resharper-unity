<idea-plugin>
  <dependencies>
    <module name="intellij.platform.frontend"/>

    <!-- region Generated dependencies - run `Generate Product Layouts` to regenerate -->
    <plugin id="com.intellij.platform.images"/>
    <plugin id="com.intellij.resharper.unity"/>
    <module name="intellij.grid.core.impl"/>
    <module name="intellij.grid.impl"/>
    <module name="intellij.platform.debugger.impl.frontend"/>
    <module name="intellij.rd.ide.model.generated"/>
    <module name="intellij.rd.platform"/>
    <module name="intellij.rider"/>
    <module name="intellij.rider.plugins.unity.debugger.textureVisualizer"/>
    <!-- endregion -->
  </dependencies>

  <extensions defaultExtensionNs="com.intellij">
    <xdebugger.nodeLinkActionProvider implementation="intellij.rider.plugins.unity.debugger.textureVisualizer.frontend.UnityTextureLinkProvider"/>
    <statistics.counterUsagesCollector implementationClass="intellij.rider.plugins.unity.debugger.textureVisualizer.frontend.TextureDebuggerCollector"/>
  </extensions>
</idea-plugin>
