<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
  </config>
  <packageSources>
    <clear />
    <!-- <add key="jbproxy.nuget.org" value="http://dotnet-nuget-org-proxy.labs.intellij.net/api/v2" /> -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="globalPackagesFolder" value="{FALLBACK_PLACEHOLDER}"/>
  </packageSources>
  <packageRestore>
    <!-- Allow NuGet to download missing packages -->
    <add key="enabled" value="True" />

    <!-- Automatically check for missing packages during build in Visual Studio -->
    <add key="automatic" value="True" />
  </packageRestore>
  <fallbackPackageFolders>
    <clear />
    <add key="MagicFallbackFolder" value="{FALLBACK_PLACEHOLDER}" />
  </fallbackPackageFolders>
</configuration>