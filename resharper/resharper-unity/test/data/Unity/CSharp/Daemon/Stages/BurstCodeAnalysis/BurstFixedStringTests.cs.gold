namespace BurstDiscardTests
{
    public class BurstDiscardTests
    {
        [BurstCompile]
        struct BurstDiscardTest1 : IJob
        {
            public void Execute()
            {
                F();
                D();
            }

            [BurstDiscard]
            void F()
            {
                var current = new object();
            }

            void D()
            {
                var current = |new object()|(0);
            }
        }

