Shader "Custom/Test" {
    Properties {
        _Color ("Color", Color) = (1,1,1,1)
        _MainTex ("Albedo (RGB)", 2D) = "white" {}
        _Glossiness ("Smoothness", Range(0,1)) = 0.5
        _Metallic ("Metallic", Range(0,1)) = 0.0
    }
    SubShader {
        Tags { "RenderType"="Opaque" }
        LOD 200
        
        CGPROGRAM
        #pragma domain |domain|(0)

        // Use shader model 3.0 target, to get nicer looking lighting
        #pragma target 3.0

        sampler2D _MainTex;

        struct Input {
            float2 uv_MainTex;
        };

        half _Glossiness;
        half _Metallic;
        |fixed4|(1) |_Color|(2);

        // Add instancing support for this shader. You need to check 'Enable Instancing' on materials that use the shader.
        // See https://docs.unity3d.com/Manual/GPUInstancing.html for more information about instancing.
        // #pragma instancing_options assumeuniformscaling
        |UNITY_INSTANCING_BUFFER_START|(3)(|Props|(4))
            // put more per-instance properties here
        |UNITY_INSTANCING_BUFFER_END|(5)(|Props|(6))

        void surf (Input IN, inout |SurfaceOutputStandard|(7) |o|(8)) {
        
            #if SHADER_TARGET > 40
                // Albedo comes from a texture tinted by color
                |fixed4|(9) |c|(10) = tex2D |(_MainTex, IN.uv_MainTex, 2)|(11) * _Color;
                o.Albedo = c.rgb;
                // Metallic and smoothness come from slider variables
                o.Metallic = _Metallic;
                o.Smoothness = _Glossiness;
                o.Alpha = c.a;
            #endif
        }
        ENDCG
    }
    FallBack "Diffuse"
}
