0271: CG_CONTENT '\r\n\r\n        #pragma surface surf Lambert\r\n\r\n        sampler2D _MainTex;\r\n        half4 _Color;\r\n\r\n        struct Input\r\n        {\r\n            float2 uv_MainTex;\r\n        };\r\n\r\n        void surf (Input IN, inout SurfaceOutput o)\r\n        {\r\n            half4 c = tex2D (_MainTex, IN.uv_MainTex);\r\n            o.Albedo = _Color.rgb * c.rgb;\r\n            o.Alpha = _Color.a * c.a;\r\n        }\r\n\r\n        '
